{
    "name": "gamemoney/gamemoney-sdk",
    "type": "library",
    "keywords": ["gamemoney", "gamemoney-sdk"],
    "license": "MIT",
    "description": "gamemoney",
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*",
        "ext-json": "*",
        "symfony/validator": "*",
        "guzzlehttp/guzzle": ">=6.5.5"
    },
    "require-dev": {
        "phpunit/phpunit": ">=6.5.14",
        "phpunit/php-code-coverage": "*",
        "php-mock/php-mock-phpunit": ">=2.6.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {"Gamemoney\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}

{
    "name": "gamemoney/gamemoney-sdk",
    "type": "library",
    "keywords": ["gamemoney", "gamemoney-sdk"],
    "license": "MIT",
    "description": "gamemoney",
    "require": {
        "php": ">=8.1",
        "ext-openssl": "*",
        "ext-json": "*",
        "symfony/validator": "*",
        "guzzlehttp/guzzle": ">=6.5.5"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.5",
        "phpunit/php-code-coverage": "*",
        "php-mock/php-mock-phpunit": "^2.14",
        "friendsofphp/php-cs-fixer": "^3.92"
    },
    "autoload": {
        "psr-4": {"Gamemoney\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}

{
    "name": "gamemoney/gamemoney-sdk",
    "type": "library",
    "keywords": ["gamemoney", "gamemoney-sdk"],
    "license": "MIT",
    "description": "gamemoney",
    "require": {
        "php": ">=5.6",
        "ext-openssl": "*",
        "ext-json": "*",
        "symfony/validator": "*",
        "guzzlehttp/guzzle": ">=4.1.4 <7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "phpunit/php-code-coverage": "*",
        "php-mock/php-mock-phpunit": "^1.1",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {"Gamemoney\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    }
}
